{
  "name": "law-transcribed",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "cross-env DEBUG=lt3:* LOG_LEVEL=debug ENABLE_TRACING=true ENABLE_DEBUG_LOGGING=true ENABLE_MEMORY_MONITORING=true ENABLE_DEBUG_DASHBOARD=true next dev",
    "dev:profile": "node --inspect=9229 --cpu-prof --cpu-prof-dir=./debug/profiles ./node_modules/next/dist/bin/next dev",
    "build": "node --require ./node-preload.js ./node_modules/next/dist/bin/next build",
    "build:analyze": "SET ANALYZE=true&& node --require ./node-preload.js ./node_modules/next/dist/bin/next build",
    "build:sourcemaps": "cross-env GENERATE_SOURCEMAP=true next build",
    "start": "next start",
    "start:debug": "cross-env PRODUCTION_DEBUG=true next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:load": "k6 run k6-load-test.js",
    "test:coverage": "vitest run --coverage",
    "debug:dashboard": "node -e \"require('./lib/debug/dashboard.ts').lt3Dashboard.start()\"",
    "debug:providers": "node scripts/debug/debug-providers.js",
    "debug:providers:ai": "node scripts/debug/debug-providers.js ai",
    "debug:providers:asr": "node scripts/debug/debug-providers.js asr",
    "debug:performance": "node scripts/debug/performance-test.js",
    "debug:performance:full": "node scripts/debug/performance-test.js full",
    "debug:performance:db": "node scripts/debug/performance-test.js db",
    "debug:performance:ai": "node scripts/debug/performance-test.js ai",
    "debug:performance:asr": "node scripts/debug/performance-test.js asr",
    "debug:heap": "node scripts/debug/heap-snapshot.js snapshot",
    "debug:heap:analyze": "node scripts/debug/heap-snapshot.js analyze",
    "debug:heap:monitor": "node scripts/debug/heap-snapshot.js monitor",
    "debug:memory": "node --expose-gc scripts/debug/heap-snapshot.js snapshot gc-enabled",
    "profile:cpu": "node --cpu-prof --cpu-prof-dir=./debug/profiles ./node_modules/next/dist/bin/next dev",
    "profile:heap": "node --heap-prof --heap-prof-dir=./debug/profiles ./node_modules/next/dist/bin/next dev",
    "clean:debug": "rimraf debug/profiles debug/snapshots debug/reports debug/sessions debug/*.log",
    "analyze": "npm run build:analyze && npx @next/bundle-analyzer"
  },
